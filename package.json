    "@10up/stylelint-config": "^1.0.9",
    "@babel/core": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.4.2",
    "@wordpress/dom": "^3.9.0",
    "@wordpress/env": "^5.8.0",
    "@wordpress/eslint-plugin": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.28.1",
    "browser-sync-webpack-plugin": "^2.2.2",
    "browserslist": "^4.21.4",
    "caniuse-db": "^1.0.30001019",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.1",
    "css-loader": "^3.4.1",
    "cssnano": "^4.1.10",
    "cypress": "^11.2.0",
